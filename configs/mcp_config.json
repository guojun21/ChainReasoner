{
  "mcpServers": {
    "iqs-search": {
      "type": "http",
      "url": "https://iqs-mcp.aliyuncs.com/mcp-servers/iqs-mcp-server-search",
      "api_key": "8TthsyoOfnvvHI8AZXE1GkAwyyveGZQ2OTg2MDdmZg",
      "tools": ["common_search"],
      "description": "阿里云 IQS 完整搜索（LLM 优化，多源融合）"
    },
    "iqs-litesearch": {
      "type": "http",
      "url": "https://iqs-mcp.aliyuncs.com/mcp-servers/iqs-mcp-server-litesearch",
      "api_key": "8TthsyoOfnvvHI8AZXE1GkAwyyveGZQ2OTg2MDdmZg",
      "tools": ["web_search"],
      "description": "阿里云 IQS 轻量搜索（速度快）"
    },
    "iqs-readpage": {
      "type": "http",
      "url": "https://iqs-mcp.aliyuncs.com/mcp-servers/iqs-mcp-server-readpage",
      "api_key": "8TthsyoOfnvvHI8AZXE1GkAwyyveGZQ2OTg2MDdmZg",
      "tools": ["readpage_basic", "readpage_scrape"],
      "description": "阿里云 IQS 网页内容抓取"
    },
    "mcp-deepwiki": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-deepwiki@latest"]
    },
    "trends-hub": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-trends-hub@1.6.0"]
    },
    "arxiv-mcp-server": {
      "type": "stdio",
      "command": "uv",
      "args": ["tool", "run", "arxiv-mcp-server", "--storage-path", "./paper_storage"]
    },
    "pozansky-stock-server": {
      "type": "stdio",
      "command": "uvx",
      "args": ["pozansky-stock-server"]
    },
    "worldbank-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["worldbank-mcp"],
      "description": "World Bank Open Data MCP Server"
    },
    "mcp-server-hotnews": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@wopal/mcp-server-hotnews"]
    },
    "web-search": {
      "type": "stdio",
      "command": "npx",
      "args": ["open-websearch@1.1.5"]
    },
    "biomcp": {
      "type": "stdio",
      "command": "uv",
      "args": ["run", "--with", "biomcp-python", "biomcp", "run"]
    },
    "searxng": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "mcp-searxng"],
      "env": {
        "SEARXNG_URL": "https://searx.stream"
      }
    },
    "brave-search": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-brave-search"],
      "env": {
        "BRAVE_API_KEY": "BSAKXNAlEhm9LNuh1fQEiL8AlrCFIO8"
      }
    }
  }
}
